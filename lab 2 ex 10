#include <iostream>
#include <cmath>
using namespace std;
double* alocare(int n)
{
	double* v = new(nothrow) double[n];
	if (v == nullptr)
	{
		cout << "alocare esuata!" << endl;
		exit(1);
	}
	return v;
}
void citire(int m, double* a)
{
	for (int i = 0; i < m; i++)
		cin >> *(a + i);
}
void afisare(int m, double* a)
{
	for (int i = 0; i < m; i++)
		cout << *(a + i) << " ";
}
double* suma(int n, double* a, double* u)
{
	double* s = alocare(n);
	for (int i = 0; i < n; i++)
		*(s + i) = *(a + i) + *(u + i);
	return s;
}
double* diferenta(int m, double* a, double* b)
{
	double* d = alocare(m);
	for (int i = 0; i < m; i++)
		*(d + i) = *(a + i) - *(b + i);
	return d;
}
double* produsscalar(int m, double* a, double* b)
{
	double* ps = 0;
	for (int i = 0; i < m; i++)
		ps = ps + (*(a + i) * *(b + i));
	return ps;
}
double* produscuscalar(int m, double* a)
{
	double x;
	cin >> x;
	for (int i = 0; i < m; i++)
		*(a + i) = x * *(a + i);
	return a;
}
double* opus(int m, double* a)
{
	for (int i = 0; i < m; i++)
		*(a + i) = -*(a + 1);
	return a;
}
double* normaeuclidiana(int m, double* a)
{
	double* x;
	x = 0;
	for (int i = 0; i < m; i++)
		x = x + (*(a + i) * *(a + i));
	return sqrt(x);
}


int main()
{
	int n;
	cin >> n;
	double* a, * b;
	a = alocare(n);
	b = alocare(n);
	citire(n, a);
	afisare(n, a);

	afisare(n, suma(a, b, n));

}
