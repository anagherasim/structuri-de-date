#include <iostream>
using namespace std;
double* alocare(int n)
{
	double* v = new(nothrow) double [n];
	if (v == nullptr)
	{
		cout << "alocare esuata!" << endl;
		exit(1);
	}
	return v;
}
void citire(int m, double* a)
{
	for (int i = 0; i < m; i++)
		cin >> *(a+i);
}
void afisare(int m, double* a)
{
	for (int i = 0; i < m; i++)
		cout << *(a + i) << " ";
}
double* suma(int n, double* a[], double* u)
{
	double* s = alocare(n);
	for (int i = 0; i < n; i++)
		*(s + i) = a[i] + *(u + i);
	return s;
}
//refac
int diferenta(int m, double* a, int b[])
{
	int c[];
	for (int i = 0; i < m; i++)
		*(c + i) = *(a + i) - *(b + i);
		return c;
}
int produsscalar(int m, int a[], int b[])
{
	int ps = 0;
	for (int i = 0; i < m; i++)
		ps = ps + *(a + i) * *(b + i);
		return ps;
}
int produscuscalar(int m, int a[])
{
	int x;
	cin >> x;
	for (int i = 0; i < m; i++)
		*(a + i) = x * *(a + i);
	return a;
}
int opus(int m, int a[])
{
	for (int i = 0; i < m; i++)
		*(a + i) = -*(a + 1);
	return a;
}



int main()
{
	double* u;
	u = alocare(n);
	citire(u, n);
	afisare(u, n);

	afisare(n, suma(u, w, n));

}
